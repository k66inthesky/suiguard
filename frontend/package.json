{
  "name": "suiaudit-frontend",
  "private": true,
  "version": "0.0.1",
  "description": "Website for AI-powered code audit solution on SUI",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix && prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prettier:check": "prettier -c --ignore-unknown .",
    "prettier:fix": "prettier -w --ignore-unknown .",
    "prettier:fix:watch": "onchange '**' -i -f add -f change -j 5 -- prettier -w --ignore-unknown {{file}}",
    "eslint:check": "eslint --max-warnings=0 .",
    "eslint:fix": "pnpm run eslint:check --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mysten/bcs": "^1.9.2",
    "@mysten/dapp-kit": "0.19.8",
    "@mysten/seal": "^0.9.3",
    "@mysten/slush-wallet": "^0.1.0",
    "@mysten/sui": "1.44.0",
    "@mysten/wallet-standard": "^0.13.0",
    "@mysten/walrus": "^0.8.3",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.12",
    "react": "^19.1.2",
    "react-dom": "^19.1.2",
    "react-router-dom": "^7.4.1",
    "react-spinners": "^0.17.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}
